<?xml version="1.0"?>
<Container version="2">
  <Name>jackett</Name>
  <Repository>linuxserver/jackett:latest</Repository>
  <Registry>https://hub.docker.com/r/linuxserver/jackett</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project/>
  <Overview>Jackett</Overview>
  <Category/>
  <WebUI>http://[IP]:[PORT:9117]</WebUI>
  <TemplateURL>https://gitee.com/liofee/docker-templates/raw/master/liofee.fun/jackett.xml</TemplateURL>
  <Icon>https://gitee.com/liofee/docker-templates/raw/master/liofee.fun/img/jackett-icon.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="WebUI" Target="9117" Default="" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false">9117</Config>
  <Config Name="TimeZone" Target="TZ" Default="" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">Asia/Shanghai</Config>
  <Config Name="Home" Target="/liofee" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/disk2/liofee/</Config>
  <Config Name="Config" Target="/config" Default="" Mode="rw" Description="" Type="Path" Display="advanced" Required="true" Mask="false">/mnt/user/appdata/jackett</Config>
  <Config Name="PUID" Target="PUID" Default="" Mode="" Description="" Type="Variable" Display="advanced" Required="true" Mask="false">99</Config>
  <Config Name="PGID" Target="PGID" Default="" Mode="" Description="" Type="Variable" Display="advanced" Required="true" Mask="false">100</Config>
  <Config Name="UMASK" Target="UMASK" Default="" Mode="" Description="" Type="Variable" Display="advanced" Required="true" Mask="false">022</Config>
</Container>